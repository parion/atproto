{
  "lexicon": 1,
  "id": "app.bsky.graph.getMutedKeywords",
  "defs": {
    "main": {
      "type": "query",
      "description": "What keywords are the requester's account muting?",
      "parameters": {
        "type": "params",
        "properties": {
          "limit": {"type": "integer", "minimum": 1, "maximum": 100, "default": 50},
          "cursor": {"type": "string"}
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["keywords"],
          "properties": {
            "cursor": {"type": "string"},
            "keywords": {
              "type": "array",
              "items": {"type": "string"}
            }
          }
        }
      }
    }
  }
}
